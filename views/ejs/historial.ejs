<h2 style="color: crimson;">Vista Productos</h2>

<% if(productos.length > 0){ %>
  <div class="table-responsive">
   
    <table class="table table-dark">

        <tr style="color: yellow;"><th>Nombre</th><th>Precio</th><th>Foto</th></tr>
        <% for (const producto of productos){ %>
            <tr>
                <td><%=producto.nombre%></td>
                <td><%=producto.precio%></td>
                <td><img src="<%=producto.foto%>" style="height:50 px; width: 50px;" ></td>
            </tr>
            <%};%>
    </table>
    
  </div>
  <%};%>
